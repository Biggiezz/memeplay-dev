{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "rewrites": [
    {
      "source": "/pacman-game-(.*)",
      "destination": "/games/templates/pacman-template/index.html?game=pacman-game-$1"
    }
  ]
}

