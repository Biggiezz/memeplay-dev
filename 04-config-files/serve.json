  "cleanUrls": false,
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/pacman-game-:gameId",
      "destination": "/games/templates-v2/pacman-template/index.html?game=pacman-game-:gameId"
    },
    {
      "source": "/playmode-:gameId",
      "destination": "/play-v2.html?game=playmode-:gameId"
    },
    {
      "source": "/games/templates-v2",
      "destination": "/games/templates-v2/index.html"
    },
    {
      "source": "/games/templates-v2/",
      "destination": "/games/templates-v2/index.html"
    },
    {
      "source": "/telegram-mini-app",
      "destination": "/telegram mini app/telegram-mini-app.html"
    },
    {
      "source": "/docs.html",
      "destination": "/02-html-pages/docs.html"
    }
  ]
}

